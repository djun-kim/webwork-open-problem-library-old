## Section1('6.3')
## Problem1('13')

##DIFFICULTY('easy')
##  Evaluate expressions of the form log base a of a to some power
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;
$a1 = random(2,9,1);
$b1 = random(2,15,1);
$c1 = random(2,3,1);
$d1 = random(3,5,1);
$e1 = $c1**$d1;
$f1 = random(2,9,1);
$g1 = random(2,15,1);
$h1 = random(4,5,1);
$i1 = random(3,5,1);
$j1 = $h1**$i1;

TEXT(EV2(<<EOT));
Evaluate the following expressions.
$PAR
(a)  \( \log_ { $a1 } $a1 ^ {$b1} = \) \{ans_rule(10) \}
EOT

$ans = $b1;
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
$PAR
(b)  \( \log_ { $c1 } $e1 = \) \{ans_rule(10) \}
EOT

$ans = $d1;
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
$PAR
(c)  \( \log_ { $h1 } $j1 = \) \{ans_rule(10) \}
EOT

$ans = $i1;
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
$PAR
(d)  \( \log_ { $f1 } $f1 ^ {$g1} = \) \{ans_rule(10) \}
EOT

$ans = $g1;
ANS(num_cmp($ans));

ENDDOCUMENT();        # This should be the last executable line in the problem.;
