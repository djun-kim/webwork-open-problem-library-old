############

TEXT(beginproblem());


foreach $i (0..2) {

   foreach $j (0..2) {
   		$a[$i][$j] = random(-4,4,1);
   		$b[$i][$j] = random(-4,4,1);
   	}
}


$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

\{ mbox( 'If \(A=\)', display_matrix([[$a[0][0], $a[0][1], $a[0][2]],
                                      [$a[1][0], $a[1][1], $a[1][2]],
                                      [$a[2][0], $a[2][1], $a[2][2]]]),
         'and \(B=\)', display_matrix([[$b[0][0], $b[0][1], $b[0][2]],
                                       [$b[1][0], $b[1][1], $b[1][2]],
                                       [$b[2][0], $b[2][1], $b[2][2]]]) ) \}
$BR
\{ mbox( 'Then \(AB=\)', answer_matrix(3,3,5) ) \}
$BR
\{ mbox( 'and \(BA=\)', answer_matrix(3,3,5) ) \}

END_TEXT

foreach $i (0..2) {

   foreach $j (0..2) {
        $sum = 0;
        foreach $k (0..2) {
           $sum = $sum + $a[$i][$k]*$b[$k][$j];
        } 
   		ANS(num_cmp($sum)  );
   	}
}
foreach $i (0..2) {

   foreach $j (0..2) {
        $sum = 0;
        foreach $k (0..2) {
           $sum = $sum + $b[$i][$k]*$a[$k][$j];
        } 
   		ANS(num_cmp($sum)  );
   	}
}
ENDDOCUMENT() ;        







