DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl", 
"PGgraphmacros.pl", 
"PGmatrixmacros.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$NO_SPACE = '@{}';

$a11 = non_zero_random(-3, 3, 1);
$a12 = random(-3, 3, 1);
$a13 = random(-3, 3, 1);

$rank = random(1, 3, 1);

if ( $rank == 1) { 
	$b = random(-3, 3, 1); 
	$a21 = $b * $a11;
	$a22 = $b * $a12;
	$a23 = $b * $a13;
	$c = non_zero_random(-3, 3, 1);
	$a31 = $c * $a11;
	$a32 = $c * $a12;
	$a33 = $c * $a13;
}

if ( $rank == 2) {
	$a21 = 0;
	$a22 = non_zero_random(-3, 3, 1);
	$a23 = random(-3, 3, 1);
	$b = random(-3, 3, 1);
	$c = random(-3, 3, 1);
	$a31 = $b*$a11 + $c*$a21;
	$a32 = $b*$a12 + $c*$a22;
 	$a33 = $b*$a13 + $c*$a23;
}

if ( $rank == 3) { 
	$a21 = 0;
        $a22 = non_zero_random(-3, 3, 1);
        $a23 = random(-3, 3, 1);
        $b = random(-3, 3, 1);
        $c = random(-3, 3, 1);
        $a31 = $b*$a11 + $c*$a21;
        $a32 = $b*$a12 + $c*$a22;
        $a33 = $b*$a13 + $c*$a23 + 1;
}

$ans11 = $a11 * $a11 + $a12 * $a21 + $a13 * $a31;
$ans12 = $a11 * $a12 + $a12 * $a22 + $a13 * $a32;
$ans13 = $a11 * $a13 + $a12 * $a23 + $a13 * $a33;
$ans21 = $a21 * $a11 + $a22 * $a21 + $a23 * $a31;
$ans22 = $a21 * $a12 + $a22 * $a22 + $a23 * $a32;
$ans23 = $a21 * $a13 + $a22 * $a23 + $a23 * $a33;
$ans31 = $a31 * $a11 + $a32 * $a21 + $a33 * $a31;
$ans32 = $a31 * $a12 + $a32 * $a22 + $a33 * $a32;
$ans33 = $a31 * $a13 + $a32 * $a23 + $a33 * $a33;

BEGIN_TEXT

\{ mbox( 'If \(A=\)', display_matrix([[$a11, $a12, $a13], [$a21, $a22, $a23], [$a31, $a32, $a33]])  ) \}
$BR
\{ mbox( 'Then \( {\rm rank} A = \) ', ans_rule(5), ', and' )\}
$BR
\{ mbox( '\(A^2=\)', answer_matrix(3,3,10) ) \}

END_TEXT

ANS(num_cmp($rank));
ANS(num_cmp($ans11));
ANS(num_cmp($ans12));
ANS(num_cmp($ans13));
ANS(num_cmp($ans21));
ANS(num_cmp($ans22));
ANS(num_cmp($ans23));
ANS(num_cmp($ans31));
ANS(num_cmp($ans32));
ANS(num_cmp($ans33));



