DOCUMENT() ;       

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGdiffeqmacros.pl",
) ;

$a = random(2,9,1) ;

$m = random(-9,9,1) ;
$n = random(-9,9,1) ;
$p1 = random(-4,4,1) ;

$p = $p1*(($a)**2-1)+$m;

$c3 = $p1;
$c1 = ($m+$n-$p1*($a+1))/2;
$c2 = $m -$c1 -$c3;

$ans = "$c1*e^x + $c2*e^(-x) + $c3*e^($a*x)";

TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 0 ;

BEGIN_TEXT

Find \( y \) as a function of \( x \) if $BR 
\[ y''' -$a y'' -y'+$a y =  0,\]
\( y(0) = $m, \ \ y'(0) = $n, \ \ y''(0) = $p .\) $BR
\(y(x) = \) \{ans_rule(100)\}

END_TEXT

ANS(fun_cmp($ans)) ;

ENDDOCUMENT() ;       




