DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(2,4,1);
$b1 = random(5,9,1);

BEGIN_TEXT
Evaluate the limit \[ \lim_{ x \to 0} \frac {\sin $a1 x } { $b1 x } \]
Answer: \{ans_rule(10) \}
$BR
END_TEXT

ANS(num_cmp("$a1/$b1"));

