TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=random(4,8,1);


BEGIN_TEXT
How many different ways can a race with $a runners be completed? 
(Assume there is no tie.)
$BR
Your answer is : \{ans_rule(25) \}
$BR
END_TEXT

$ans = fact($a);
ANS(num_cmp($ans));

