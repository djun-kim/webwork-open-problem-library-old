## DBsubject('Calculus')
## DBchapter('Techniques of Integration')
## DBsection('Integration by Partial Fractions')
## KEYWORDS('integration', 'partial fractions')
## TitleText1('Calculus: Early Transcendentals')
## EditionText1('5')
## AuthorText1('Stewart')
## Section1('7.4')
## Problem1('18')
## Author('')
## Institution('UCSB')

DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=2*$a;
$c=-1*$a;

BEGIN_TEXT

$PAR
Evaluate the integral
\[\int {\frac{$a x^2 + $b x + $c}{x^3-x}}\, dx\]

$PAR
Note:  Use an upper-case "C" for the constant of integration.

\{ans_rule(45)\}

END_TEXT

ANS(fun_cmp("-1*$c*ln(x)+1/2*ln(x-1)*$b+1/2*ln(x-1)*$a+1/2*ln(x-1)*$c+1/2*$a*ln(x+1)+1/2*ln(x+1)*$c-1/2*ln(x+1)*$b+C+c", var=>["x","C"], params=>["c"], limits=>[[2,10],[-10,10]]));

ENDDOCUMENT();
