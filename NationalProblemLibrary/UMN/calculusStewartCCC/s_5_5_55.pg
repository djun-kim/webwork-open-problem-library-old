Context("Numeric");

$a = list_random(4,9,16,25,36);

$ans = Compute("2*sqrt($a)-2");

#####################################################################

Context()->texStrings;
BEGIN_TEXT
Evaluate the definite integral
\[
\int_e ^{e^{$a}} \!\!\! \frac{dx}{x \sqrt{\ln x}}.
\]
Answer: \{ans_rule(20) \}
END_TEXT
Context()->normalStrings;

#####################################################################

ANS( $ans->cmp() );

