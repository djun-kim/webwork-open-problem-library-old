##DESCRIPTION
##KEYWORDS('functions', 'inverse functions')
##  Find the inverse of a function
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$b = random(20,40,1);
$c = random(2,9,1);
$d = random(100,150,1);
$e = random(2,9,.1);
$f = random(20,40,1);

$ans1 = ceil(.5*((log$b)/(log$a))**2);
$ans2 = ceil($d**(sqrt(1/$c)));
$ans3 = ceil((1/3)*((log$f)/(log$e))**2);
$ans4 = ceil($f**(sqrt($c/$a)));

BEGIN_TEXT

Determine the smallest integer \(x\) that satisfies the given inequality.
$BR $BR 
\( $a^{\sqrt{2 x}} > $b \) $BR 
\( x > \) \{ans_rule(20) \}
$BR $BR
\( x^{\sqrt{$c}} > $d \) $BR
\( x > \) \{ans_rule(20) \}
$BR $BR
\( $e^{\sqrt{3 x}} > $f \) $BR
\( x > \) \{ans_rule(20) \}
$BR $BR 
\( x^{\sqrt{\frac{$a}{$c}}} > $f \) $BR
\( x > \) \{ans_rule(20) \}
END_TEXT

ANS(num_cmp($ans1,mode=>'strict'));
ANS(num_cmp($ans2,mode=>'strict'));
ANS(num_cmp($ans3,mode=>'strict'));
ANS(num_cmp($ans4,mode=>'strict'));

ENDDOCUMENT();        # This should be the last executable line in the problem.
