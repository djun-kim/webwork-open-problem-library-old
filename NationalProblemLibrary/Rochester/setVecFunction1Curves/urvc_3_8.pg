DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(1,5,1);
$b1 = random(-4,4,1);
$b2 = $b1*$b1;
$c1 = non_zero_random(-5,5,1);
$pi = arccos(-1);


BEGIN_TEXT
$PAR
 If 
 \( \mathbf{r}(t)= (\sqrt{t+$a1})\mathbf{i}+ \frac{t^2-{$b2}}{t-$b1}\mathbf{j}+
 \sin($c1 \pi t)\mathbf{k} \). $BR
 Then $BR 
 \( \displaystyle \lim_{ t \rightarrow 1} \mathbf{r}(t) \)=
  \{ans_rule(10)\} \( \mathbf{i}+\) \{ans_rule(10)\}\( \mathbf{j}+\)
  \{ans_rule(10)\}\( \mathbf{k}\).
$PAR $BR
END_TEXT


$ans0 =  ($a1+1)**.5 ;
$ans1 =  $b1+1 ;
$ans2 =  sin($c1*$pi) ;


ANS(num_cmp($ans0));
ANS(num_cmp($ans1));
ANS(num_cmp($ans2));

ENDDOCUMENT();        # This should be the last executable line in the problem.
