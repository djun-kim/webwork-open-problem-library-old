##DESCRIPTION
##KEYWORDS('matrices', 'matrix operations', 'product')
##
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl", 
"PGgraphmacros.pl",
"PGmatrixmacros.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$NO_SPACE = '@{}';

$a11 = random(-10,10,1);
$a12 = random(-10,10,1);
$a21 = random(-10,10,1);
$a22 = random(-10,10,1);

$b11 = random(-10,10,1);
$b12 = random(-10,10,1);
$b21 = random(-10,10,1);
$b22 = random(-10,10,1);

$ans11 = $a11 * $b11 + $a12 * $b21 ;
$ans12 = $a11 * $b12 + $a12 * $b22 ;
$ans21 = $a21 * $b11 + $a22 * $b21 ;
$ans22 = $a21 * $b12 + $a22 * $b22 ;

BEGIN_TEXT

\{ mbox( 'If \(A=\)', display_matrix([[$a11, $a12], [$a21, $a22]]),  
	 'and \(B=\)', display_matrix([[$b11, $b12], [$b21, $b22]]) ) \}
$BR
\{ mbox( 'Then \(AB=\)', answer_matrix(2,2,10) )\}


END_TEXT

ANS(num_cmp($ans11));
ANS(num_cmp($ans12));
ANS(num_cmp($ans21));
ANS(num_cmp($ans22));



ENDDOCUMENT();       # This should be the last executable line in the problem.
