##DESCRIPTION
##KEYWORDS('matrix', 'product')
##
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGgraphmacros.pl",
"PGmatrixmacros.pl", 
"PGnumericalmacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a11 = non_zero_random(-5,5,1);
$a12 = random(-5,5,1);
$a13 = random(-5,5,1);
$a21 = random(-5,5,1);
$a22 = random(-5,5,1);
$a23 = random(-5,5,1);
$a31 = random(-5,5,1);
$a32 = random(-5,5,1);
$a33 = random(-5,5,1); 

$b1 = non_zero_random(-5,5,1);
$b2 = random(-5,5,1);
$b3 = random(-5,5,1);

$ans1 = $a11*$b1 + $a12*$b2 + $a13*$b3;
$ans2 = $a21*$b1 + $a22*$b2 + $a23*$b3;
$ans3 = $a31*$b1 + $a32*$b2 + $a33*$b3;


BEGIN_TEXT

Compute the following product. 
$BR
\{ mbox([ display_matrix([[$a11, $a12, $a13], [$a21, $a22, $a23], [$a31, $a32, $a33]]), 
display_matrix([[$b1], [$b2], [$b3]]), '\(=\)', answer_matrix(3,1,10) ]) \}

END_TEXT

ANS(num_cmp($ans1));
ANS(num_cmp($ans2));
ANS(num_cmp($ans3));

ENDDOCUMENT();       # This should be the last executable line in the problem.

