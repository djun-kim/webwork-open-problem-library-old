##DESCRIPTION
##KEYWORDS('matrix', 'entry')
##
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGgraphmacros.pl",
"PGmatrixmacros.pl", 
"PGnumericalmacros.pl",
"PGauxiliaryFunctions.pl",
"PGmorematrixmacros.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$A = new Matrix(3,4);

foreach $i (1..3) { 
	foreach $j (1..4) { 
		$a[$i][$j] = random(-20,20,1);
		$A -> assign($i, $j, $a[$i][$j]); 
	}
}

$j1 = random(3,4,1);
$j2 = random(1,4,1);
$j3 = random(1,2,1);

BEGIN_TEXT

\{ mbox( 'if \(A=\)', display_matrix($A), ',' ) \}
$BR 
determine the following entries: 
$BR
\(a_{1 $j1} = \) \{ans_rule(5)\}, 
$BR
\(a_{2 $j2} = \) \{ans_rule(5)\}, 
$BR
\(a_{3 $j3} = \) \{ans_rule(5)\}.

END_TEXT

ANS(num_cmp($a[1][$j1]));
ANS(num_cmp($a[2][$j2]));
ANS(num_cmp($a[3][$j3]));

ENDDOCUMENT();       # This should be the last executable line in the problem.

