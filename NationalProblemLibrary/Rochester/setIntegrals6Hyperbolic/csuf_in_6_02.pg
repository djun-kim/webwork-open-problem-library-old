##DESCRIPTION
##KEYWORDS('integral', 'hyperbolic')
##  Find the integral derivative of a function involving hyperbolic trigonometric function(s).
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$b = random(2,9,1)*random(-1,1,2);
$ans = "sinh($a*x+$b)/$a";

BEGIN_TEXT
Evaluate the integral. $PAR
 
\( \displaystyle \int \cosh ($a x + $b)  dx = \) \{ans_rule(50) \} \(+C\).
$BR
END_TEXT

ANS(fun_cmp($ans, mode=>"antider"));

ENDDOCUMENT();        # This should be the last executable line in the problem.
