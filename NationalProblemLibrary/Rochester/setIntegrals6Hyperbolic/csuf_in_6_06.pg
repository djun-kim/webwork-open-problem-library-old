##DESCRIPTION
##KEYWORDS('integral', 'hyperbolic')
##  Find the integral derivative of a function involving hyperbolic trigonometric function(s).
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(0,4,1);
$b = random($a+1,9,1);
$c = random(2,5,1);
$c2 = $c*$c;
$ac = $a*$c;
$bc = $b*$c;
$ans = (ln($bc+sqrt($bc*$bc+1))-ln($ac+sqrt($ac*$ac+1)))/$c;

BEGIN_TEXT
Evaluate the integral. $PAR
 
\( \displaystyle \int_$a^$b \frac{dt}{\sqrt{$c2 t^2+1}} = \) \{ans_rule(50) \}.
$BR
END_TEXT

ANS(num_cmp($ans));

ENDDOCUMENT();        # This should be the last executable line in the problem.
