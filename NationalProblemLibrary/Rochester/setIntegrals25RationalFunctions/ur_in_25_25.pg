&DOCUMENT;
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);


TEXT(beginproblem());

$m=random(1,3,1);
$n=random(1,6,1);
$a=random(1,4,1);
$c=random(1,5,1);
$b=$a+$c;
$p=$m+$n;
$q=$n*$a+$m*$b;
$l=$a+$b;
$k=$a*$b;
$funct="-$p*x+$m*ln($a+(e^x))+$n*ln($b+(e^x))";
$r=$q-$p*$l;
$s=$p*$k;
TEXT(EV2(<<EOT));

Evaluate the integral. 
$BR
$BR \[ \int\frac{$r e^x-$s}{e^{2x}+$l e^x+$k} dx \]
$BR \{ans_rule( 60) \}
EOT
$ans=$funct;
ANS(fun_cmp($ans, mode=>"antider"));

&ENDDOCUMENT



