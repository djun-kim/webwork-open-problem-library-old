&DOCUMENT;
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);


TEXT(beginproblem());

$m=random(1,3,1);
$n=random(1,6,1);
$a=random(-3,3,1);
$c=random(1,5,1);
$b=$a+$c;
$p=$m+$n;
$q=$n*$a+$m*$b;
$l=$a+$b;
$k=$a*$b;
$soln=$m*ln(1.5)+$n*ln(($c+3)/($c+2));
$r=2-$a;
$s=3-$a;

TEXT(EV2(<<EOT));


Evaluate the integral. 
$BR
$BR \[ \int_{$r}^{$s} \frac{$p x+$q}{x^2+$l x+$k} dx \]
$BR \{ans_rule(60) \}
EOT
ANS(num_cmp($soln));

&ENDDOCUMENT


