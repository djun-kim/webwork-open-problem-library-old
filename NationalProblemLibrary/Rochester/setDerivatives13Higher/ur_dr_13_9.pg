DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$an = random(2,9,1);
$as = random(-1,1,2);
$a  = $an * $as;

$n = random(4,4,1);

$c = 1;
$f_n = 1;
while ($c < 1+$n) {
  $f_n = $f_n * $c;
  $c = $c + 1;
  }

TEXT(EV2(<<EOT));
\( \frac{d^{$n}}{dx^{$n}} ( \frac{$a x^{$n}}{1-x} ) \)
= \{ ans_rule(20) \}
$BR
Note:  There is a way of doing this problem without using the quotient rule
$n times.
EOT

$ans = "$a * $f_n (1-x)**(-1-$n)";

ANS(fun_cmp($ans));

ENDDOCUMENT();        # This should be the last executable line in the problem.

