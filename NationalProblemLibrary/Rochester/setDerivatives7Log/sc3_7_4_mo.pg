DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(2,5,1);
$b1 = random(2,8,1);
$c1 = random(2,4,1);
$x1 = random(1,5,1);
$deriv1 = -$a1*sin($b1*ln($x1))*$b1/$x1;
$funct1 = "-$a1*sin($b1*ln(x))*$b1/x";

TEXT(EV2(<<EOT));
If \( f(x) = $a1 \cos($b1\ln(x))  \), find \( f'( x ) \).
$BR $BR \{ans_rule(50) \}
$BR
EOT

$ans = $funct1;
ANS(fun_cmp($ans));

TEXT(EV2(<<EOT));
Find \( f'( $x1 ) \).
$BR $BR \{ans_rule(50) \}
$BR
EOT

$ans = $deriv1;
ANS(num_cmp($ans));

ENDDOCUMENT();        # This should be the last executable line in the problem.
