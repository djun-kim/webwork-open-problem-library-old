##DESCRIPTION
##Problem 4, section 1.6 Fundamentals of Complex Analysis for Mathematics,
##Science, and Engineering by E.B. Saff and A.D. Snider
##ENDDESCRIPTION


DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGcomplexmacros.pl"
);

TEXT(beginproblem());

$check = new_checkbox_multiple_choice();

$check -> qa(
"Which of the given sets are domains?",#bc
"\(0<\vert z - 2 \vert < 3\)",
"\(\vert Arg\ z \vert < \frac{\pi}{4}\)"
);

$check -> extra(
"\(\vert z-1+i \vert \le 3 \)",
"\(-1<\ Im\ z \le 1\)",
"\(\vert z \vert \ge 2 \)",
"\((Re\ z)^2>1\)"
);

BEGIN_TEXT
\{ $check->print_q \}
\{ $check->print_a \}
END_TEXT	

ANS(checkbox_cmp( $check->correct_ans ));

ENDDOCUMENT();        # This should be the last executable line in the problem.

