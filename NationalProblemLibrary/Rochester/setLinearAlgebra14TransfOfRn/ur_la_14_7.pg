##DESCRIPTION
##KEYWORDS('vector space', 'linear transformation', 'matrix', 'onto', 'one-to-one', 'kernel', 'range')
##
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGgraphmacros.pl",
"PGnumericalmacros.pl",
"PGstatisticsmacros.pl", 
"PGmatrixmacros.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$rank = random(1,3,1);
$ker = 4-$rank;
if ($rank == 3) { $onto = "yes"; } else { $onto = "no"; } 

$a11 = non_zero_random(-5,5,1);
$a12 = random(-5,5,1);
$a13 = non_zero_random(-5,5,1);
$a14 = random(-5,5,1);

if ($rank == 1) { 

	$m = random(-2.5,2.5,1);
	$a21 = $m * $a11;
 	$a22 = $m * $a12;
	$a23 = $m * $a13;
	$a24 = $m * $a14; 

	$n = random(-3,3,1);
	$a31 = $n * $a11;
        $a32 = $n * $a12;
        $a33 = $n * $a13;
        $a34 = $n * $a14;
}

if ($rank == 2) { 

	$m = random(-2.5,2.5,1); 
        $a21 = $m * $a11;
        $a22 = $m * $a12;
        $a23 = $m * $a13;
	$d = non_zero_random(-5,5,.5);
        $a24 = $m * $a14+$d;

        $n = non_zero_random(-3,3,1);
	$p = non_zero_random(-3,3,1);
        $a31 = $n * $a11 + $p * $a21;
        $a32 = $n * $a12 + $p * $a22;
        $a33 = $n * $a13 + $p * $a23;
        $a34 = $n * $a14 + $p * $a24;

}

if ($rank == 3) {

        $m = random(-2.5,2.5,1);
        $a21 = $m * $a11;
        $d = non_zero_random(-5,5,.5);
        $a22 = $m * $a12 + $d;
        $a23 = $m * $a13;
        $a24 = $m * $a14;

        $n = non_zero_random(-3,3,1);
        $p = non_zero_random(-3,3,1);
        $a31 = $n * $a11 + $p * $a21;
        $a32 = $n * $a12 + $p * $a22;
        $a33 = $n * $a13 + $p * $a23;
        $e = non_zero_random(-5,5,.5);
        $a34 = $n * $a14 + $p * $a24 + $e;
 
}

BEGIN_TEXT

The matrix 
$BR
\{ mbox( '\(A=\)', display_matrix([[$a11, $a12, $a13, $a14], 
				  [$a21, $a22, $a23, $a24], 
				  [$a31, $a32, $a33, $a34]]) ) \} 
$BR
is a matrix of a linear transformation \( T: {\mathbb R}^k \rightarrow {\mathbb R}^n \) 
where 
$BR
\( k= \)\{ans_rule(5)\}, \( \ \) \( n= \)\{ans_rule(5)\}, $BR 
\(\mbox{dim(Ker}(T))= \)\{ans_rule(5)\}, \( \ \) \( \mbox{dim(Range}(T))= \)\{ans_rule(5)\}. $BR
Is \(T\) onto? (enter YES or NO) \{ans_rule(5)\}. $BR
Is \(T\) one-to-one? (enter YES or NO) \{ans_rule(5)\}.

END_TEXT

ANS(num_cmp(4));
ANS(num_cmp(3));
ANS(num_cmp($ker));
ANS(num_cmp($rank));
ANS(str_cmp($onto));
ANS(str_cmp("no"));

ENDDOCUMENT();       # This should be the last executable line in the problem.

