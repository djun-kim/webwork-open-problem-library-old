##DESCRIPTION
##KEYWORDS('True False Sample question')
##this problem is also located in the directory setFunctions0Evaluate
##problem 2 section 1.6 of Discrete Math by Kenneth Rosen
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$a = random(2,5,1);
$b = $a**2;
$c = random(1,9,1);
$d = random(1,9,1);

$tf = new_select_list();

#$tf = new_pop_up_select_list();   #For popups!


$tf -> qa (
"\( f(n)= \pm n \)",
"N",
"\( f(n)= \sqrt{n^2 +$d} \)",
"Y",
"\( f(n)= \frac{1}{n^2-$b} \)",
"N",
"\( f(n)= 1/(n^2+$c) \)",
"Y"
);

$tf ->choose(4);

BEGIN_TEXT
$PAR
Determine whether f is a function from \( \mathbb{Z} \) to \( \mathbb{R}
\). $BR
Enter "Y" for yes and "N" for no. $BR
\{ $tf -> print_q \}
$PAR
END_TEXT

ANS(str_cmp( $tf -> ra_correct_ans ) );

ENDDOCUMENT();        # This should be the last executable line in the problem.;
