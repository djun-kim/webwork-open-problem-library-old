TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

@a = NchooseK(12,12);
for ($i=0; $i<12; $i++){
	$a[$i] = $a[$i] - 7 ; 
	if (abs($a[$i])<2) { 
        	$a[$i] = $a[$i] + 10;
	}
} 


$NO_SPACE = '@{}';	

BEGIN_TEXT

Write the system 
\[ \left\{"\{"\} \begin{array}{r${NO_SPACE}r${NO_SPACE}r${NO_SPACE}r${NO_SPACE}r} 
        &   $a[1] y & + $a[2] z & = & $a[3] \cr
$a[4] x & + $a[5] y &           & = & $a[7] \cr
$a[8] x & + $a[9] y & + $a[10] z & = & $a[11]  
\end{array} \right. \] 
in matrix form. 
$BR
\{ mbox([ answer_matrix(3,3,5), ' ', display_matrix([['\(x\)'], ['\(y\)'], ['\(z\)']]), '\(=\)',
answer_matrix(3,1,5) ]) \} 
 
END_TEXT

ANS(num_cmp(0));
ANS(num_cmp($a[1]));
ANS(num_cmp($a[2]));
ANS(num_cmp($a[4]));
ANS(num_cmp($a[5]));
ANS(num_cmp(0));
ANS(num_cmp($a[8]));
ANS(num_cmp($a[9]));
ANS(num_cmp($a[10]));
ANS(num_cmp($a[3]));
ANS(num_cmp($a[7]));
ANS(num_cmp($a[11]));


