Context("Numeric");

$a = Real( random(2,9,1) );
$b = Real( random(2,8,1) );
if ($a == $b) { $b = 9; } 
$coeff = 1 / $b;
$antiderivative = Compute( "$a *ln(abs(ln($b * x)))" );
$integrand = Compute("{$a }/{x ln ($b x)} ");
##############################################################
#
#  Text
#
#

Context()->texStrings;
BEGIN_TEXT

Find $BBOLD one $EBOLD indefinite integral (anti-derivative):
$PAR \[ \int $integrand \; dx \]
$BR $BR \{ans_rule( 30) \}
$BR
END_TEXT
