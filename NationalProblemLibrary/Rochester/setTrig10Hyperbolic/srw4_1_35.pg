DOCUMENT(); # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$a1 = random(2,8,1);

TEXT(EV2(<<EOT));
$PAR
(a) \( \sinh ( $a1 ) = \) \{ans_rule(20) \}
EOT

$ans = sinh($a1);
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
$PAR
(b) \( \sinh ( -$a1 ) = \) \{ans_rule(20) \}
EOT

$ans = sinh(-$a1);
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
$PAR
(c) \( \cosh ( $a1 ) = \) \{ans_rule(20) \}
EOT

$ans = cosh($a1);
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
$PAR
(d) \( \cosh ( -$a1 ) = \) \{ans_rule(20) \}
EOT

$ans = cosh(-$a1);
ANS(num_cmp($ans));
ENDDOCUMENT(); # This should be the last executable line in the problem.;
