#DESCRIPTION
##  Algebra problem:  partial fractions
##ENDDESCRIPTION

##KEYWORDS('algebra', 'partial fractions')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=random(2,25,1);

BEGIN_TEXT
How many fraction terms are there in the partial fraction decomposition of
\( \displaystyle  \frac{x^3+x^2+$a}{x(2x-5)^3(x^2+2x+5)^2}?\)
$BR
Your answer is \{ans_rule(10) \}.
$BR
END_TEXT

$ans1 = 6;

ANS(num_cmp($ans1));

ENDDOCUMENT();        # This should be the last executable line in the problem.

