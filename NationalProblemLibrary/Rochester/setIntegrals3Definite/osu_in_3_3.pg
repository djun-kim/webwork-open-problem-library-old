TEXT(beginproblem());
BEGIN_TEXT
\( \displaystyle \int_$a^$b \frac{$c x^2 + $d}{x^2}\, dx\) =  \{ ans_rule()\}
$PAR
END_TEXT

ANS(num_cmp($answ));

