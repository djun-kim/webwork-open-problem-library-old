##KEYWORDS('algebra, percent')


DOCUMENT();     
loadMacros("PGbasicmacros.pl",     
           "PGchoicemacros.pl",     
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl"    
);     

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(100,300,10);
$b=random(20,75,5);

BEGIN_TEXT

\($a\) is \($b\) percent of what number?
$BR $BR 
Your answers is :  \{ans_rule(10) \}
$BR$BR
END_TEXT

ANS(num_cmp($a*100/$b));

ENDDOCUMENT();
