##DESCRIPTION
##question 24 section 2.3 of Discrete Math by Kenneth Rosen
##KEYWORDS('Standard Example','greatest common divisor')
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers=0;


$a = random(1,5,1);
$b = random(1,5,1);
$c = random(1,5,1);
$d = random(1,5,1);
$e = random(1,5,1);
$f = random(1,5,1);
$g = random(1,12,1);
$h = random(1,12,1);
$j = random(1,12,1);

@prime = ('2','3','5','7','11','13');
@slice = NchooseK(6,3);
@sprime = @prime[@slice];
@p = num_sort(@sprime);
@q = @*-1;
@r = num_sort(@b);
 
$a1 = 2**$a*3**$b*5**$c;
$b1 = 2**$d*3**$e*5**$f;
$a2 = 2**$g*3*5*7*11*13;
$b2 = $p[0]**$g*$p[1]**$h*$p[2]**$j*17;


BEGIN_TEXT
What are the greatest common divisors of the following pairs of integers? 
$BR
(a) \( 2^$a * 3^$b * 5^$c \) and \( 2^$d * 3^$e * 5^$f \) 
$BR
\{ ans_rule(20) \}
$PAR
END_TEXT

ANS(num_cmp( gcd($a1,$b1) ) );

BEGIN_TEXT
(b) \( 2^{$g} * 3 * 5 * 7 * 11 * 13\) and \( $p[0]^{$g} * $p[1]^{$h} * $p[2]^{$j} * 17 \)
$BR
\{ ans_rule(20) \}
$PAR
END_TEXT

ANS(num_cmp( gcd($a2,$b2) ) );

BEGIN_TEXT
(c) \( 2^$a * 7\) and \( 5^$b * 13 \) \{ ans_rule(20) \}
$PAR
END_TEXT

ANS(num_cmp( 1 ) );

ENDDOCUMENT();        # This should be the last executable line in the problem.;
