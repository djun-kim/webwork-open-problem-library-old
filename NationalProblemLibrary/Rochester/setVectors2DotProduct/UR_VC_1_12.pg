TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(-10, 10);
$b = random(-10, 10);
$c = random(-10, 10);
$d = random(-10, 10);
$e = random(-10, 10);
$f = random(-10, 10);

$ans = $a*$d + $b*$e + $c*$f;

BEGIN_TEXT
$PAR
If \( {\bf a} \) = ($a, $b, $c) and \( {\bf b} \) = ($d, $e, $f),

$PAR find \( {\bf a \cdot \ b} \) = \{ ans_rule \}.
END_TEXT
ANS(num_cmp($ans));

