);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(-10, 10);

$dfdx = 1;
$dfdy = -1;

BEGIN_TEXT
$PAR
Find the first partial derivatives of \( f(x, y) = \sin(x - y) \) at the point
($a, $a).
$PAR
A. \( f_x($a, $a) = \) \{ ans_rule(30) \}
$PAR
B. \( f_y($a, $a) = \) \{ ans_rule(30) \}
END_TEXT
ANS(num_cmp($dfdx));
ANS(num_cmp($dfdy));

