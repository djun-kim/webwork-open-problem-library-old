           "PGchoicemacros.pl",     
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl"    
);     

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(120,420,10);
$b=random(20,75,5);

BEGIN_TEXT
What is \($b\) percent of \($a\)?.
$BR $BR 
Your answers is :  \{ans_rule(10) \}
$BR$BR
END_TEXT

ANS(std_num_cmp($a*$b/100));

