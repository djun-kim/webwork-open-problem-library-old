DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 0;

$ml = new_match_list();
$ml -> qa (
         "\( \phi = \frac{\pi}{3} \)",
	 "Cone",
	 "\( \theta = \frac{\pi}{3} \)",
	 "Half plane",
	 "\( \rho = 4 \)",
	 "Sphere",
	 "\( r =4 \)",
	 "Circular Cylinder",
	 "\( z = r^2 \)",
	 "Elliptic or Circular Paraboloid",
	 "\( \rho = 2\cos(\phi ) \)",
	 "Sphere",
	 "\( \rho \cos(\phi )= 4\)",
	 "Plane",
	 "\( r^2 + z^2 =16 \)",
	 "Sphere",
	 "\( r = 2\cos(\theta ) \)",
	 "Circular Cylinder"
);
$ml ->choose(9);

BEGIN_TEXT
$PAR
Match the given equation with the verbal description of the surface:
$PAR
\{ $ml -> print_a \}
$PAR
\{ $ml -> print_q \}
 

END_TEXT

ANS( str_cmp( $ml->ra_correct_ans )   ) ;


ENDDOCUMENT();        # This should be the last executable line in the problem.