DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl", 
"PGgraphmacros.pl", 
"PGmatrixmacros.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

$a11 = non_zero_random(-5,5,1);
$a22 = non_zero_random(-5,5,1); 
$a33 = non_zero_random(-5,5,1);

$a12 = 0;
$a13 = 0;
$a21 = 0;
$a23 = 0;
$a31 = 0;
$a32 = 0;

$det = $a11*$a22*$a33;

$ans11 = ($a22 * $a33)/$det;
$ans12 = 0;
$ans13 = 0;
$ans21 = 0;
$ans22 = ($a11 * $a33)/$det;
$ans23 = 0;
$ans31 = 0;
$ans32 = 0;
$ans33 = ($a11 * $a22)/$det;

BEGIN_TEXT

\{ mbox( 'If \(A=\)', display_matrix([[$a11, $a12, $a13], [$a21, $a22, $a23], [$a31, $a32, $a33]]) ) \}
$BR
\{ mbox( 'Then \(A^{-1}=\)', answer_matrix(3,3,5) ) \} 

END_TEXT

ANS(num_cmp($ans11));
ANS(num_cmp($ans12));
ANS(num_cmp($ans13));
ANS(num_cmp($ans21));
ANS(num_cmp($ans22));
ANS(num_cmp($ans23));
ANS(num_cmp($ans31));
ANS(num_cmp($ans32));
ANS(num_cmp($ans33));

ENDDOCUMENT();       # This should be the last executable line in the problem.