loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGgraphmacros.pl",
"PGnumericalmacros.pl",
"PGstatisticsmacros.pl", 
"PGmatrixmacros.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a11 = non_zero_random(-9,9,1);
$a12 = non_zero_random(-9,9,1);
$a13 = non_zero_random(-9,9,1);

$a21 = non_zero_random(-9,9,1);
$a22 = non_zero_random(-9,9,1);
$a23 = non_zero_random(-9,9,1);

$a31 = non_zero_random(-9,9,1);
$a32 = non_zero_random(-9,9,1);
$a33 = non_zero_random(-9,9,1);

$u1 = non_zero_random(-5,5,1);
$u2 = non_zero_random(-5,5,1);
$u3 = non_zero_random(-5,5,1);


$b1 = $a11*$u1+$a12*$u2+$a13*$u3;
$b2 = $a21*$u1+$a22*$u2+$a23*$u3;
$b3 = $a31*$u1+$a32*$u2+$a33*$u3;

BEGIN_TEXT

$BR
\{ mbox( 'Let \(A =\)', display_matrix([[$a11, $a12, $a13], 
[$a21, $a22, $a23], [$a31, $a32, $a33]]), 
	 '.' ) \}  
$BR
Define the linear transformation \( T: {\mathbb R}^3 \rightarrow {\mathbb R}^3\) as \(T(x) = Ax\). $BR
\{ mbox( 'Find the images of  \(u  =\)', display_matrix([[$u1], [$u2], [$u3]]), 
         'and \(v =\)', display_matrix([["\(a\)"],["\(b\)"],["\(c\)"]]),
	 ' under \(T\) .' ) \}  $BR

$BR
\{ mbox( '\( T(u) = \)', answer_matrix(3,1,10) ) \} 

$BR
\{ mbox( '\( T(v) = \)', answer_matrix(3,1,10) ) \} 


END_TEXT

ANS(num_cmp($b1));
ANS(num_cmp($b2));
ANS(num_cmp($b3));

$x = "(a*$a11+$a12*b+$a13*c)";
$y = "(a*$a21+$a22*b+$a23*c)";
$z = "(a*$a31+$a32*b+$a33*c)";

ANS(fun_cmp($x, vars=>["a","b","c"]));
ANS(fun_cmp($y, vars=>["a","b","c"]));
ANS(fun_cmp($z, vars=>["a","b","c"]));


ENDDOCUMENT();       # This should be the last executable line in the problem.

