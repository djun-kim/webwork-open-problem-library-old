## DESCRIPTION
## KEYWORDS('vectors', 'image', 'linear', 'transformation')
##
## ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGmatrixmacros.pl",
"Parser.pl",
"Generic.pl",
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 0;


$a12 = non_zero_random(-5,5,1);
$a13 = non_zero_random(-5,5,1);
$a14 = non_zero_random(-5,5,1);

$a22 = 1;
$a23 = non_zero_random(-5,5,1);
$a24 = non_zero_random(-5,5,1);

$c = non_zero_random(-3,3,1);
$d = non_zero_random(-3,3,1);

$a31 = $c;
$a32 = $c*$a12 + $d*$a22;
$a33 = $c*$a13 + $d*$a23;
$a34 = $c*$a14 + $d*$a24;



Context("Vector");

$vec=Matrix->new_from_array_ref( [[1, $a12, $a13, $a14],[0,$a22 ,$a23, $a24],[$a31, $a32, $a33, $a34]]);


BEGIN_TEXT

\{ mbox( 'Let \( A = \)', display_matrix($vec) ) \}
$BR
$BR
Find a  vector \(w \) in \( {\mathbb R}^3 \) that is not in the image of 
the transformation \( x\mapsto Ax \).
$BR
$BR
\( w = \) \{ans_rule()\}

END_TEXT


sub check{
  my $stu=shift();
  $x1=$stu->extract(1); 
  $x2=$stu->extract(2);
  $x3=$stu->extract(3);
  
  $c2=$x2/$a22;
  $c1=$x1- $c2*$a12;
  $c3=$c1*$a31 + $c2*$a32 - $x3;

  $c3 != 0;
}

ANS(generic_cmp("23",type => 'Vector', length => 3, checker => ~~&check));

ENDDOCUMENT();       # This should be the last executable line in the problem.