TEXT(beginproblem());
BEGIN_PROBLEM();

$a = random(2,7,1);
$aa = $a**2;
$antiderv = "x arcsin($a x)+(1/$a)(1-$aa x^2)^(1/2)";

BEGIN_TEXT
Evaluate the indefinite integral.
$PAR
\( \displaystyle\int \arcsin($a x) \;dx \) 
             = \{ans_rule(60)\} \( + C\). 
END_TEXT

ANS(fun_cmp($antiderv, mode=>"antider", vars=>"x", limits=>[-1/$a,1/$a]));

END_PROBLEM();
