BEGIN_PROBLEM();

$b = random(2,9,1);
$a = $b**3;
$bb = $b**2;
$db = $b * 2;
$antiderv = "${bb}x^2 sin(${b}x)+${db}x cos(${b}x)-2sin(${b}x)";

BEGIN_TEXT
Evaluate the indefinite integral.
$PAR
\( \displaystyle\int ${a}x^2 \cos (${b}x) \; dx \) 
             = \{ans_rule(45)\} \( + C\). 
END_TEXT

