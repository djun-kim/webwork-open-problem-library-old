TEXT(beginproblem());
BEGIN_PROBLEM();

$aa = random(3,7,1);
$antiderv = exp(1/$aa)*$aa-2*exp(1/$aa)*$aa**2+2*exp(1/$aa)*$aa**3-2*$aa**3;

BEGIN_TEXT
Evaluate the definite integral.
$PAR
 \( \displaystyle \int_0^1 x^2 \sqrt[$aa]{e^x}\; dx\)  = \{ans_rule(60) \}
END_TEXT

ANS(fun_cmp($antiderv, mode=>"antider", vars=>"x", limits=>[0,1]));

END_PROBLEM();
