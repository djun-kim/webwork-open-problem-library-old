DOCUMENT();

loadMacros(
   "PG.pl",
   "PGbasicmacros.pl",
   "PGchoicemacros.pl",
   "PGanswermacros.pl",
   "PGauxiliaryFunctions.pl",
   "PGunion.pl",        # Union College utilities
   "PGcourse.pl",       # Customization file for the course
);

TEXT(beginproblem());
BEGIN_PROBLEM();

$a = random(1, 4, 1);
$b = random(3, 8,1);
$c = random(2,7,1);
$d = random(3,9,1);
$f = random(1,6,1);
$g = random(4, 12, 1);
$r1 = random(1,5,1);
$r2 = random(-7,-1,1);
$h1 = $a*(($r1)**2) -$b*($r1) +$c;
$dh1 = 2*$a*$r1 - $b;
$l1 = $d*(($r1)**2) + $f*($r1) - $g;
while ($d*(($r1)**2) + $f*($r1) - $g == 0)
{
$g = random(4, 12, 1);
}
$dl1 = 2*$d*$r1 + $f;
$ans = (($l1)*($dh1) - ($h1)*($dl1))/(($l1)**2);


BEGIN_TEXT
Let
\[f(x) =  \frac{$a x^2 - $b x + $c}{$d x^2 + $f x - $g}. \]
Evaluate \(f'(x)\) at \(x = $r1\).
$PAR
\( f'($r1) \) = \{ans_rule(50) \}
END_TEXT

$showpartialcorrectanswers = 1;
ANS(num_cmp($ans));

END_PROBLEM();
ENDDOCUMENT();

