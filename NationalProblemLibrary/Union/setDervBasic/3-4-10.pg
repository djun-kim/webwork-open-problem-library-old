$PAR
\(\displaystyle{\frac{d}{dx}(x^{$a/$b})}\) = \{ans_rule(20) \}
END_TEXT

$expnum=$a-$b;
$ans = "($a/$b)x^($expnum/$b)";
ANS(fun_cmp($ans));

