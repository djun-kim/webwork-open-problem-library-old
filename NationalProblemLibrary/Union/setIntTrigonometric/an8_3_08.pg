##DESCRIPTION
##KEYWORDS('integrals', 'trigonometric integrals')
##ENDDESCRIPTION

# K. Lesh, Union College, edited 10/1/03

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
   "PG.pl",
   "PGbasicmacros.pl",
   "PGchoicemacros.pl",
   "PGanswermacros.pl",
   "PGauxiliaryFunctions.pl",
   "PGunion.pl",        # Union College utilities
   "PGcourse.pl",       # Customization file for the course
);

TEXT(beginproblem());
BEGIN_PROBLEM();

$n = random(3,9,2);
$power2 = $n+2;
$antiderv = "(1/$power2)(sec(x))^$power2 - (1/$n)(sec(x))^$n";

BEGIN_TEXT
Evaluate the indefinite integral.
$BR
\( \displaystyle\int \tan^{3}(x)\sec^{$n}(x)\,dx \) 
             = \{ans_rule(60)\} \( + C\). 
END_TEXT

ANS(fun_cmp($antiderv, mode=>"antider", vars=>"x", limits=>[0,1]));

END_PROBLEM();
ENDDOCUMENT();
