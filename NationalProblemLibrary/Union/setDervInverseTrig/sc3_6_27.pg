TEXT(beginproblem());
BEGIN_PROBLEM();

$a1 = random(2,8,1);
$b1 = random(2,8,1);
$b1squared=$b1**2;
$funct1 = "$a1*${b1}e^x/(1+${b1squared}(e^x)^2)";

BEGIN_TEXT
Let \( f(x) = $a1 \arctan($b1 e^x)\). Find \(f'(x)\).
$BR 
\(f'(x) = \)\{ans_rule(45) \}
END_TEXT
includePGfile("inverse-trig-notation-text.pg");

$showPartialCorrectAnswers = 1;
$ans = $funct1;
ANS(fun_cmp($ans));

END_PROBLEM();
