DOCUMENT();

loadMacros(
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
);

$showPartialCorrectAnswers = 1;
$a = random(2,9,1);
$av = 2*$a**3-9*$a*$a**2+18*$a**2*$a-6*$a**3*ln($a);
$bv = 2*(1+$a)**3 -9*$a*(1+$a)**2+18*$a**2*(1+$a)-6*$a**3*ln($a+1);
$ans = "$bv - $av";

TEXT(beginproblem());

BEGIN_TEXT

Evaluate the definite integral. $BR
\[\int_0^1 \frac{dx}{\sqrt{x}+ $a\sqrt[3]{x}}\]
$BR
Answer: \{ ans_rule(20)\}
$BR
END_TEXT

ANS(num_cmp($ans));

ENDDOCUMENT();
