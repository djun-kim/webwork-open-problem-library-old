DOCUMENT();

loadMacros(
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);
$showPartialCorrectAnswers = 1;

$AA = random(3,6,1);
$CC = random(3,7,1);
if ($CC==$AA) {$CC++;}
$ab = $AA^2;
$ac = $CC^2;
$ans = "(pi/2)^2/(2*$CC)";

TEXT(beginproblem());

BEGIN_TEXT
Evaluate the following integral.
$PAR
\[ \int_0^{$AA/$CC}\frac{\arcsin \left(\frac{$CC}{$AA}x\right)}{\sqrt{$ab - $ac
x^2}}\, dx\] 
$BR Answer: \{ ans_rule(35)\}
$BR
END_TEXT

ANS(num_cmp($ans));

ENDDOCUMENT();
