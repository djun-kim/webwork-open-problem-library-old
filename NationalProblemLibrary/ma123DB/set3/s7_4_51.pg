##KEYWORDS('integrals', 'partial fractions')
##DESCRIPTION
## Evaluate an indefinite integral.
##ENDDESCRIPTION

## AmberHolden tagged
## Shotwell cleaned

## DBsubject('Calculus')
## DBchapter('Techniques of Integration')
## DBsection('Integration by Partial Fractions')
## Date('6/3/2002')
## Author('')
## Institution('')
## TitleText1('Calculus: Early Transcendentals')
## EditionText1('4')
## AuthorText1('Stewart')
## Section1('7.4')
## Problem1('51`')

DOCUMENT();
loadMacros(
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);


TEXT(beginproblem());

$m=random(1,3,1);
$n=random(1,6,1);
$a=random(1,4,1);
$c=random(1,5,1);
$b=$a+$c;
$p=$m+$n;
$q=$n*$a+$m*$b;
$l=$a+$b;
$k=$a*$b;
$funct="-$p*x+$m*ln($a+(e^x))+$n*ln($b+(e^x))";
$r=$q-$p*$l;
$s=$p*$k;
$num = nicestring([$r, -$s], ['e^x', '']);

BEGIN_TEXT
Evaluate the integral. 
$BR
$BR \[ \int\frac{$num}{e^{2x}+$l e^x+$k} dx \]
$BR Answer: \{ans_rule( 60) \} \(+C\)
END_TEXT
$ans=$funct;
ANS(fun_cmp($ans, mode=>'antider'));

ENDDOCUMENT();



