## Section1('7.4')
## Problem1('43')


DOCUMENT();

loadMacros(
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
);

$showPartialCorrectAnswers = 1;

$a = random(4,9,1);
$b = random(5,9,1);
if ($a==$b) {$b++;}
$t1 = 2*($a+$b)^(7/2)/7-4*$b*($a+$b)^(5/2)/5+2*$b^2*($a+$b)^(3/2)/3;
$t2 = 2*$b^(7/2)/7-4*$b*$b^(5/2)/5+2*$b^2*$b^(3/2)/3;
$ans = "($t1-$t2)/$a^3";

TEXT(beginproblem());

BEGIN_TEXT
Evaluate the definite integral. $BR
\[ \int_0^1 x^2\sqrt{$a x + $b}\,dx\]
$BR
Answer: \{ ans_rule(20)\}

END_TEXT

ANS(num_cmp($ans));

ENDDOCUMENT();

