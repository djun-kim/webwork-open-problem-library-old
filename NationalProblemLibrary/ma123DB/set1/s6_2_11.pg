DOCUMENT();

loadMacros(
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

$showPartialCorrectAnswers = 1;

$aa = random(2,6);
$a2 = $aa*$aa;


TEXT(beginproblem());

BEGIN_TEXT
The region between the graphs of \(y=x^2\) and \(y=$aa x\)
is rotated around the line \(y=$a2\).
$BR$BR
The volume of the resulting solid is: \{ ans_rule()\}
END_TEXT

ANS(num_cmp("(pi*$aa^5)/5"));

ENDDOCUMENT();
