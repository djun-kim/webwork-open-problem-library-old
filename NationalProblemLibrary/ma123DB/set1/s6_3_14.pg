DOCUMENT();

loadMacros(
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

$showPartialCorrectAnswers = 1;

$aa = random(2,6);
$a2 = $aa*$aa;
$soln = "pi*($aa^4)/6";

TEXT(beginproblem());

BEGIN_TEXT
The region between the graphs of \(x=y^2\) and \(x=$aa y\)
is rotated around the line \(y=$aa\).
$BR$BR
The volume of the resulting solid is:  \{ ans_rule()\}
END_TEXT

ANS(num_cmp($soln));

ENDDOCUMENT();
