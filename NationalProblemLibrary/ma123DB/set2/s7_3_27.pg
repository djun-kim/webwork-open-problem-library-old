DOCUMENT();

loadMacros(
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

$showPartialCorrectAnswers = 1;

$a = random(2,6,1);
$b = random(5,10,1);

$a2 = 2*$a;
$c = $a**2 + $b**2;


TEXT(beginproblem());

BEGIN_TEXT
$BR
\( \displaystyle \int \frac{1}{x^2+$a2 x + $c}\,dx\) =   \{ ans_rule(45)\} \(+\) \(C\)

END_TEXT

ANS(fun_cmp("(1/$b)*arctan((x+$a)/$b)",mode=>'antider'));

ENDDOCUMENT();
