DOCUMENT() ;
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
) ;

TEXT(beginproblem()) ;
$showPartialCorrectAnswers = 1 ;

$r = random(1,4,1) ;
$s = 1/ $r  ;
$m = random(2,20,1) ;
$p = random(5,9,1) ;
$d = 1/ $p  ;
$q = $p + $r ;
$n = $p/$q  ;
$c = $m - $q  ;
$f = $p*$r ;
BEGIN_TEXT

Solve the initial value problem $BR
\[ $q (t + 1) \frac{dy}{dt} - $p y =$f  t, \] $BR
for \( t > -1 \) with \( y(0) = $m. \) $BR
\( y = \) \{ans_rule(60) \} $BR


END_TEXT

$ans = "$p*t +$q  + ($c * ((t + 1)**$n))" ;


ANS(fun_cmp($ans, vars=>"t")) ;

ENDDOCUMENT() ;       

