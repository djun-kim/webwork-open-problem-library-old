#DESCRIPTION  This is a permutation type conversion problem.
#ENDDESCRIPTION

DOCUMENT();
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGkenFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 0;
@b=NchooseK(7,7);
for($i=0;$i<7;$i++){$a[$i]=$b[$i]+1;}
$c=permftoc(@a);
BEGIN_TEXT
#This is an example  using randomization
$PAR

Convert the permutation \(\begin{pmatrix}1&2&3&4&5&6&7 \\ $a[0]&$a[1]&$a[2]&$a[3]&$a[4]&$a[5]&$a[6]\end{pmatrix}\quad\)
$BR 
to standard cycle form (leave no spaces in your answer)
$BR
\{ ans_rule()\}
END_TEXT
$ans=permftoc(@a);
#$ANS =(std_str_cmp($ans));
ANS(std_str_cmp($ans));

ENDDOCUMENT(); 