##DESCRIPTION

##ENDDESCRIPTION

##KEYWORDS('vector', 'limit')

DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"extraAnswerEvaluators.pl",
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;


$a = random(-5, 5);
$b = random(-5, 5);
$c = random(1, 5);
$d = random(1, 5);

$rt = sqrt($c*($a)**2 + $d*($b)**2);
$ans = exp($rt);

BEGIN_TEXT
$PAR
Find the limit, if it exists, or type 'DNE' if it does not exist.
$PAR
\( \displaystyle \lim_{(x, y) \rightarrow ($a, $b)} e^{\sqrt{${c}x^2 + ${d}y^2}} = \) \{ ans_rule(40)
\}
END_TEXT
 
&ANS(num_cmp($ans, strings=>["DNE"]));

ENDDOCUMENT();
