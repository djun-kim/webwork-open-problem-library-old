## -*- perl -*- ##
## Line above puts emacs in perl mode
##
## Description  
##   Iterated Integrals
## EndDescription

DOCUMENT();
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "PGgraphmacros.pl",
           "Dartmouthmacros.pl");
 

## Do NOT show partial correct answers
$showPartialCorrectAnswers = 1;

## Lots of set up goes here


## Ok, we are ready to begin the problem...
##
TEXT(beginproblem());

$a = random(1, 4, 1);
$b = random(1, 4, 1);
$c = random(1, 4, 1);
$c_x = clean_scalar_string($c, "x");
$d = $a + 1;
$e = $b + 1;

BEGIN_TEXT
Evaluate the iterated integral 
\( \int_{$a}^{$d} \int_{$b}^{$e} ($c_x + y)^{-2} \: dy dx \)
$PAR
\{ ans_rule(40) \}
END_TEXT

$part1 = ln($c*$a + $c + $b);
$part2 = ln($c*$a + $c + $b + 1);
$part3 = ln($c*$a + $b);
$part4 = ln($c*$a + $b + 1);

$ans = (1/$c)*( $part1 - $part2 - $part3 + $part4 );

ANS( num_cmp( $ans ) );


ENDDOCUMENT();




