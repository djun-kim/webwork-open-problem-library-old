"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);

TEXT(EV2(<<EOT));
Evaluate
\[ \lim_{ \theta \to 0 } \frac{ \cos \theta - 1 }{ $a \sin \theta } . \]
Limit = \{ ans_rule(20) \}
$BR
EOT

$ans = 0;
ANS(num_cmp($ans));

