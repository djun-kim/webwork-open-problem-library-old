DOCUMENT();        # This should be the first executable line in the problem.

loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "extraAnswerEvaluators.pl");

$b1 = non_zero_random(2,5,1);
$e1 = non_zero_random(2,5,1);
$v1 = $b1**$e1;
$b2 = non_zero_random(6,9,1);
$e2 = non_zero_random(2,5,1);
$v2 = $b2**$e2;


TEXT(beginproblem());

TEXT(EV2(<<EOT));
Find the exact value for each expression:
$PAR
(A) \( \log_{$b1} $v1 \) = \{ans_rule(12)\}
$PAR
(B) \( \log_{$b2} \frac{1}{$v2} \) = \{ans_rule(12)\}
$PAR
EOT

$ans1 = $e1;
$ans2 = $e2;

ANS(( num_cmp($ans1, mode=>"strict"), num_cmp($ans2, mode=>"strict")) );

ENDDOCUMENT();        # This should be the last executable line in the problem.
