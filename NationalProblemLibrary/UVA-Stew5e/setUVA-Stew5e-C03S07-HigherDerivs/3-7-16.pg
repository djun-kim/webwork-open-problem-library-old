DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(7,9,2);
$b1 = random(2,6,2);
$c1 = random(3,5,2);

$x1 = random(1,5,1);

$deriv1f="((2*$a1)*($b1)*(x)+($a1)*($c1)*(x^2))/(($b1+$c1*x)**2)";
$deriv1= ((2*$a1)*($b1)*($x1)+($a1)*($c1)*($x1^2))/(($b1+$c1*$x1)**2);
$deriv2f="(((($b1+$c1*x)^2)*(2*$a1*$b1+2*$a1*$c1))-($a1*x)*(2*$b1+$c1*x)*(2*$c1)*($b1+$c1*x))/(($b1+$c1*x)^4) ";
$deriv2= (((($b1+$c1*$x1)^2)*(2*$a1*$b1+2*$a1*$c1))-($a1*$x1)*(2*$b1+$c1*$x1)*(2*$c1)*($b1+$c1*$x1))/(($b1+$c1*$x1)^4) ;

TEXT(EV2(<<EOT));
Let \( f(x) = $a1 x^2 /($b1-$c1 x)  \). $BR
Then \( f'(x) \) = \{ans_rule(40) \} $BR
and  $BR
EOT

$ans1 = $deriv1f;
$ans2 = $deriv1;

ANS(fun_cmp($ans1));
TEXT(EV2(<<EOT));
\( f''( x ) \) = \{ans_rule(40) \} $BR

EOT

$ans3 = $deriv2f;
$ans4 = $deriv2;
ANS(fun_cmp($ans3));

ENDDOCUMENT();        # This should be the last executable line in the problem.
