DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$x=random(2,9,1);
$a=random(2,3,1);
$a1 =$x**$a;
$y=random(2,9,1);
$b=random(2,3,1);
$b1=$y**$b;

BEGIN_TEXT
Find \(x\).
$BR
(a) \( \log_x $a1=$a \)
$BR
\(x=\) \{ans_rule(25) \}
$BR
$BR
(b) \( \log_x $b1=$b \)
$BR
\(x=\) \{ans_rule(25) \}
$BR
END_TEXT

$ans1 = $x;
$ans2 = $y;

ANS(num_cmp($ans1));
ANS(num_cmp($ans2));

ENDDOCUMENT();        # This should be the last executable line in the problem.
