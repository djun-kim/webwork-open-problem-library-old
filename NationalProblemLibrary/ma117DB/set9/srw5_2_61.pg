$BR
\(\cos t =\) \{ans_rule(15) \};
$BR
\(\tan t =\) \{ans_rule(15) \};
$BR
\(\cot t =\) \{ans_rule(15) \};
$BR
\(\sec t =\) \{ans_rule(15) \};
$BR
\(\csc t =\) \{ans_rule(15) \};
$BR
END_TEXT

$ans1=-4/5;
$ans2=-3/4;
$ans3=-4/3;
$ans4=-5/4;
$ans5=5/3;

