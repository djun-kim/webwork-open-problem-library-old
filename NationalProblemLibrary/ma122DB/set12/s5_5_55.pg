DOCUMENT();

loadMacros(
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGgraders.pl"
);

$showPartialCorrectAnswers = 1;

$aa = random(3,8,1);

TEXT(beginproblem());

BEGIN_TEXT
\( \displaystyle \int_1^{$aa} \frac{1}{x^2}\sqrt{1 +\frac{1}{x}}\, dx\) = \{ ans_rule()\}
END_TEXT
$ans1 = "(-2/3)*((1+1/$aa)^(3/2)-2^(3/2))";

ANS(num_cmp($ans1));

ENDDOCUMENT();
