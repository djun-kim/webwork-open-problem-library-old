DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);



$showPartialCorrectAnswers = 1;

$a = random(2,7,1);
$b = random(4,15,1);
$c = random(3,8,1);
$answ1 = "$a/($a*x-$b)-1/x-(2*x)/($c*(x^2+1))";
$answ2 = "x^(cos(x))*(cos(x)/x - sin(x)*ln(x))";

TEXT(&beginproblem);
BEGIN_TEXT
Find \(\frac{dy}{dx}\) for each of the following functions
$BR
\[y = \ln\left(\frac{$a x - $b}{x\sqrt[$c]{x^2+1}}\right)\]
$BR
\(\frac{dy}{dx}\) = \{ ans_rule(40)\}
$PAR

\[y = x^{\cos(x)}\]
$BR
\(\frac{dy}{dx}\) = \{ ans_rule(40)\}
END_TEXT

&ANS(function_cmp($answ1,"x",$b/$a+1,$b/$a+2));
&ANS(function_cmp($answ2,"x"));

ENDDOCUMENT();
