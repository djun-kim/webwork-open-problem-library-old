DOCUMENT();

loadMacros('PG.pl',
        'PGbasicmacros.pl',
        'PGchoicemacros.pl',
        'PGanswermacros.pl',
        'PGauxiliaryFunctions.pl'

);

TEXT(beginproblem()); 

$showPartialCorrectAnswers = 0;

$A = random(2,9,1);

BEGIN_TEXT

Find the area of the region bounded by \(y=\tanh x\), \(y=0\) and \(x=$A\).

$PAR

Area = \{ans_rule(40)\}.

END_TEXT

$ans = ln(cosh($A));
ANS(std_num_cmp($ans));

ENDDOCUMENT();
