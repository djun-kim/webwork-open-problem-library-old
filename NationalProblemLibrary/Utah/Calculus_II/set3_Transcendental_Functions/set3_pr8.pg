DOCUMENT();

loadMacros('PG.pl',
        'PGbasicmacros.pl',
        'PGchoicemacros.pl',
        'PGanswermacros.pl',
        'PGauxiliaryFunctions.pl'
);

TEXT(beginproblem()); 

$showPartialCorrectAnswers = 0;

$A = random(2,9,1);

BEGIN_TEXT

Find 
\[\displaystyle\int\frac{\displaystyle\cosh($A\sqrt{z})}{\displaystyle $A\sqrt{z}}\, dz.\]

$PAR

Answer: \{ans_rule(80)\}  +  C.

END_TEXT

$ans = "2/($A**2)*sinh($A*sqrt(z))";
ANS(fun_cmp($ans, [[1,2]], vars=>["z"]));

ENDDOCUMENT();
