TEXT(EV2(<<EOT));

$BR
\(\displaystyle\frac{~\frac{~\frac{a}{b}~}{c}~}{d} = \) \{ ans_rule(3) \} \(/\)
EOT

ANS(fun_cmp("a", [[1,2],[1,2],[1,2],[1,2]], vars=>["a","b","c","d"]));

TEXT(EV2(<<EOT));
 \{ ans_rule(3) \}.  
EOT
ANS(fun_cmp("b*c*d", [[1,2],[1,2],[1,2],[1,2]], vars=>["a","b","c","d"]));

TEXT(EV2(<<EOT));

$BR
\(\displaystyle\frac{a}{~\frac{b}{~\frac{c}{d}~}~} = \) \{ ans_rule(3) \} \(/\)
EOT

ANS(fun_cmp("a*c", [[1,2],[1,2],[1,2],[1,2]], vars=>["a","b","c","d"]));

TEXT(EV2(<<EOT));
 \{ ans_rule(3) \}.  
EOT
ANS(fun_cmp("b*d", [[1,2],[1,2],[1,2],[1,2]], vars=>["a","b","c","d"]));

