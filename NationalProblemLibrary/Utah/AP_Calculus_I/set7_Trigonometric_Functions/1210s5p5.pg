DOCUMENT();        # This should be the first executable line in the problem.

loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
"PGauxiliaryFunctions.pl");

TEXT(&beginproblem);

TEXT(EV2(<<EOT));
Let
\[f(x) = \tan x^2.\]
$BR
\(f'(x) = \) \{&ans_rule(45)\}.
EOT
$ans="2*x/cos(x*x)**2";
&ANS( multivar_function_cmp( $ans, ["x"], [[0.1,1]] ) );

TEXT(EV2(<<EOT));
$BR
Let
\[g(x) = \tan^2x.\]
$BR
\(g'(x) = \) \{&ans_rule(45)\}.
EOT
$ans="2tan(x)/cos(x)/cos(x)";
&ANS( multivar_function_cmp( $ans, ["x"], [[0.1,1]] ) );






