DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);


TEXT(&beginproblem);



TEXT(EV2(<<EOT));

$BR
The angle \(\frac{\pi}{6}\) equals 
\{ ans_rule(12) \} degrees.
EOT
&ANS(num_cmp(30));



TEXT(EV2(<<EOT));

$BR
And more basic trig values:

$BR
\(\cos \left(\frac{\pi}{6}\right) = \)\{ ans_rule(12) \}.
EOT
&ANS(num_cmp(sqrt(3)/2));

TEXT(EV2(<<EOT));
$BR
\(\sin\left(\frac{\pi}{6}\right) = \)\{ ans_rule(12) \}.
EOT
&ANS(num_cmp(1/2));

TEXT(EV2(<<EOT));
$BR
\(\tan\left(\frac{\pi}{6}\right) = \)\{ ans_rule(12) \}.
EOT
&ANS(num_cmp(1/sqrt(3)));









