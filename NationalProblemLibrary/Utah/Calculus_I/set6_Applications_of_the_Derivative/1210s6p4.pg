DOCUMENT();        # This should be the first executable line in the problem.

loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
"PGauxiliaryFunctions.pl");

TEXT(&beginproblem);

TEXT(EV2(<<EOT));

Suppose
\[f(t) = \sqrt{t^2+1}.\]
Then
$BR

\(f'(t)= \)
 \{&ans_rule(40)\}.

EOT
$ans="t/sqrt(t**2 + 1)";
&ANS( multivar_function_cmp( $ans, ["t"]));

TEXT(EV2(<<EOT));
$BR
\(f''(t)= \)
 \{&ans_rule(40)\}.

EOT
$ans="1/(sqrt(t**2 + 1)*(t**2 + 1))";
&ANS( multivar_function_cmp( $ans, ["t"]));

TEXT(EV2(<<EOT));
$BR
\(f'''(t)= \)
 \{&ans_rule(40)\}.

EOT
$ans="( - 3*t)/(sqrt(t**2 + 1)*(t**4 + 2*t**2 + 1))";
&ANS( multivar_function_cmp( $ans, ["t"]));








ENDDOCUMENT();        # This should be the last executable line in the problem.
