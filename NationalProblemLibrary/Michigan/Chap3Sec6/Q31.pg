DOCUMENT();
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"MathObjects.pl",
);
 
Context("Numeric");
Context()->variables->add( t=>'Real' );
Context()->flags->set( limits=>[0.1,10] );

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$b = random(2,9,1);

Context()->texStrings;
BEGIN_TEXT

Find the derivative of the function \( f(t) \), below.  It may be to
your advantage to simplify before differentiating.  
$BR
\( f(t) = \ln(\ln($a t)) + \ln(\ln $b) \)

$PAR
\( f'(t) = \) \{ ans_rule(65) \}

END_TEXT
Context()->normalStrings;


ANS( Compute( "1/(t*ln($a*t))" )->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\( f'(t) = \frac{1}{\ln($a t)}\cdot\frac1{$a t}\cdot $a = \frac1{t \ln($a t)} \)

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
