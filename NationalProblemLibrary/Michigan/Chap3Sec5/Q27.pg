DOCUMENT();
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
);
 
TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

$a = random(2,15,1);

BEGIN_TEXT

Find the derivative of 

\( z=\tan(e^{-$a w}) \)

$PAR
\( {dz\over dw} = \) \{ ans_rule(65) \}

END_TEXT


ANS( fun_cmp( "(1/(cos(e^(-1*$a*w))^2))*(-1*$a)*e^(-1*$a*w)", 'var'=>['w'] ) );

SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\[ {dz\over dw} = \frac1{\cos^2(e^{-$a w})} (-$a) e^{-$a w}
        = \frac{-$a e^{-$a w}}{\cos^2(e^{-$a w})} \]

END_SOLUTION

ENDDOCUMENT();
