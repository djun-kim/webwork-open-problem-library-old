DOCUMENT();
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
);
 
TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$b = random(2,9,1);
$b++ if ( $a == $b );

BEGIN_TEXT

Find the derivative of 
\( y = \sin($a x)\cdot\sin($b x) \)

$PAR
\( {dy\over dx} = \) \{ ans_rule(65) \}

END_TEXT


ANS( fun_cmp( "$a*cos($a*x)*sin($b*x) + $b*sin($a*x)*cos($b*x)" ) );

SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\[ y' = $a \cos($a x) \sin($b x) + $b \sin($a x) \cos($b x) \]

END_SOLUTION

ENDDOCUMENT();
