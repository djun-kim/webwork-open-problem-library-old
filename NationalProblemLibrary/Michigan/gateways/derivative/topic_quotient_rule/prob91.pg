DOCUMENT();
loadMacros(
           "PGbasicmacros.pl",
           "PGanswermacros.pl",
           "PGchoicemacros.pl",
           "PGauxiliaryFunctions.pl",
	   "MathObjects.pl",
           );

TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( t=>'Real');
Context()->variables->set( t=>{limits=>[0.65,5]});

# Context()->flags->set( limits=>[0.65,5] );

$ans = "(cos(t)*(10*t^3 - 3*t^2 - 1) - (sin(t) + sin(5))*(30*t^2 - 6*t)) / (10*t^3 - 3*t^2 - 1)^2";

$tut = htmlLink("http://instruct.math.lsa.umich.edu/cgi-bin/tutorial/" .
		"gwderivtut?pnum=291","quotient rule","");

Context()->texStrings;
BEGIN_TEXT
Differentiate \(f(t) =\displaystyle{\sin(t) + \sin(5) \over 10t^{3} - 3t^{2} - 1}\)
$BR
Answer: \( f'(t) = \) \{ans_rule(55)\}
END_TEXT
Context()->normalStrings;

# ANS(fun_cmp($ans, 'var'=>['t'], 'limits'=>[1,10]));
ANS( Compute("$ans")->cmp() );

$bsbox = MODES('TeX' => '\par{\bf Solution outline:} ',
	       'Latex2HTML' => '\par{\bf Solution outline:} ',
	       'HTML' => '<div class="gwSoln"><b>Solution outline:</b>');
$esbox = MODES('TeX' => '\par',
	       'Latex2HTML' => '\par',
	       'HTML' => '</div>');
SOLUTION($bsbox,"The solution to this problem is obtained with the ", $tut,
	 ".", $esbox);

ENDDOCUMENT();
