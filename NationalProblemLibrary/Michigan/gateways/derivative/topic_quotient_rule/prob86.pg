DOCUMENT();
loadMacros(
           "PGbasicmacros.pl",
           "PGanswermacros.pl",
           "PGchoicemacros.pl",
           "PGauxiliaryFunctions.pl",
	   "MathObjects.pl",
           );

TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( z=>'Real');
Context()->variables->set( z=>{limits=>[-8,0]});

# Context()->flags->set( limits=>[-8,0] );

$ans = "(3*z^2*(3*z^3 - 1) - 9*z^2*(z^3 + 1/2)) / (3*z^3 - 1)^2";

$tut = htmlLink("http://instruct.math.lsa.umich.edu/cgi-bin/tutorial/" .
		"gwderivtut?pnum=286","quotient rule","");

Context()->texStrings;
BEGIN_TEXT
Differentiate \(g(z) =\displaystyle{z^{3} + {1\over2} \over 3z^{3} - 1}\)
$BR
Answer: \( g'(z) = \) \{ans_rule(55)\}
END_TEXT
Context()->normalStrings;

# ANS(fun_cmp($ans, 'var'=>['z'], 'limits'=>[-8,0]));
ANS( Compute("$ans")->cmp() );

$bsbox = MODES('TeX' => '\par{\bf Solution outline:} ',
	       'Latex2HTML' => '\par{\bf Solution outline:} ',
	       'HTML' => '<div class="gwSoln"><b>Solution outline:</b>');
$esbox = MODES('TeX' => '\par',
	       'Latex2HTML' => '\par',
	       'HTML' => '</div>');
SOLUTION($bsbox,"The solution to this problem is obtained with the ", $tut,
	 ".", $esbox);

ENDDOCUMENT();
