## HHSection2('Tables of Integrals')



DOCUMENT();
loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl",
"hhAdditionalMacros.pl",
"parserFormulaUpToConstant.pl",
);
 
$showPartialCorrectAnswers = 1;
Context("Numeric");
Context()->variables->are(y=>"Real");
Context()->flags->set(reduceConstants=>0);

$a = random(2,8,1);
$asq = $a*$a;
$acu = $asq*$a;
$dfdx = Compute("y^2 cos($a y)");
$f = FormulaUpToConstant("(1/$a)y^2 sin($a y) + (2/$asq) y cos($a y) - " .
                         "(2/$acu) sin($a y) + C");

Context("Numeric");

TEXT(beginproblem());
Context()->texStrings;
Context()->texStrings;
BEGIN_TEXT

Antidifferentiate using the
table of integrals. You may need to transform the integrand
first.$BR
\( \displaystyle\int $dfdx\,dy = \)
\{ ans_rule(55) \}

END_TEXT
Context()->normalStrings;
Context()->normalStrings;

ANS( $f->cmp() );

Context()->texStrings;
Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\[ \int $dfdx\,dy = $f. \]

END_SOLUTION
Context()->normalStrings;
Context()->normalStrings;

ENDDOCUMENT();
