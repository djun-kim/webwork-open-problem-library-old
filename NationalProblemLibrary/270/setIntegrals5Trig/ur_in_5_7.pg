##DESCRIPTION
##KEYWORDS('integrals', 'trigonometric')
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

$b1 = random(11,15,1);
$b2 = random(11,15,1);
$a = random(2,5,1);
$pi = arccos(-1);
$soln = (tan($a*$pi/$b1)-tan(-$a*$pi/$b2))/$a-($pi/$b1+$pi/$b2);

BEGIN_TEXT

Evaluate the definite integral.
$BR \[ \int_{\frac{-\pi}{$b2}}^{\frac{\pi}{$b1}} \tan^2($a x) dx \]
$BR $BR \{ans_rule( 30) \}
$BR
END_TEXT

$ans = $soln;
&ANS(std_num_cmp($ans));

