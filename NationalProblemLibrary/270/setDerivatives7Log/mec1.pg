DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$an = random(2,5,1);
$as = random(-1,1,2);
$a  = $an * $as;
$b  = random(2,8,1);
$x  = random(2,5,1);

$funct1 = "$a/x";

TEXT(EV2(<<EOT));
Let \[ f(x) = $a \ln($b x) \]
$PAR
\( f'( x ) = \) \{ans_rule(40) \}
$PAR
\( f'( $x ) = \) \{ans_rule(40) \}

EOT

$ans1 = $funct1;
ANS(fun_cmp($ans1));
$ans2 = $a/$x;
ANS(num_cmp($ans2));

ENDDOCUMENT();        # This should be the last executable line in the problem.
