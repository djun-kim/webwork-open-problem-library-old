# ENDDESCRIPTION

## DBsubject('Precalculus')
## DBchapter('Compositions, Inverses, And Combinations Of Functions')
## DBsection('Composition Of Functions')
## KEYWORDS('composition','decomposition','function')
## TitleText1('Functions Modeling Change')
## EditionText1('3')
## AuthorText1('Connally')
## Section1('8.1')
## Problem1('16')
## Author('Adam Spiegler')
## Institution('Loyola University Chicago')

DOCUMENT();       

loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGgraphmacros.pl",
           "PGauxiliaryFunctions.pl",
           "extraAnswerEvaluators.pl"
           );
       
TEXT(beginproblem()); # standard preamble to each problem.


$showPartialCorrectAnswers = 1;

$p = random(2,4,1);
$m = random(2,9,1);

$f = "cos($m x)";

$k = "\cos^{$p}{($m x)}";


BEGIN_TEXT
Question 16:
$BR
$SPACE
$BR
Identify the function \( f(x) \) if
$BR
$BCENTER
\( \displaystyle k(x) = \left( f(x) \right)^$p = $k \) 
$ECENTER
$BR
$SPACE
$BR
\( f(x) = \) \{ ans_rule(30) \}
$BR
END_TEXT

ANS( fun_cmp( $f, vars=>'x' ) );

SOLUTION(EV3(<<'END_SOLUTION'));
$BR $SPACE $BR 
$BBOLD  SOLUTION $EBOLD
$BR 
The inside function is \( f(x) = \cos{( $m x)} \).
$BR
END_SOLUTION

ENDDOCUMENT();