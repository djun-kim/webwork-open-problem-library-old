DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);
$d=3*$a;
$e=(-2)*$a;
BEGIN_TEXT

$PAR
Evaluate the integral
\[\int {\frac{$d x^2 + $e}{x^2-2x-8}}\, dx\]

$PAR
Note: Use an upper-case "C" for the constant of integration.

$PAR
\{ans_rule(45)\}

END_TEXT

ANS(fun_cmp("$a*(3*x+23/3*ln(abs(x-4))-5/3*ln(abs(x+2)))+C+c", var=>["x","C"], params=>["c"], limits=>[[4.1,10],[-10,10]]));

ENDDOCUMENT();
