TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(2,10,1);
$b=random(1,10,1);
 while($b==$a) {$b=random(1,10,1)};

BEGIN_TEXT

$PAR

Differentiate \(\displaystyle G(u)= \ln{\sqrt{\frac{$a u + $b}{$a u - $b}}} \).

$PAR

\(G'(u) =\) \{ans_rule(30)\}

END_TEXT

ANS(fun_cmp(" -2*($a)/(($a)^2 u^2 - ($b)^2) ", var=>"u"));

