DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,5,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);

BEGIN_TEXT

$PAR
Evaluate the indefinite integral
\[\int {\frac{$a}{(5t+4)^{2.7}}}\, dt\]

$PAR
Note: Any arbitrary constants used must be an upper-case "C".

$PAR
\{ans_rule(30)\}

END_TEXT

ANS(fun_cmp("-1/8.5*$a/(5*t+4)^(17/10)+C+c", var=>["t","C"], param=>["c"], limits=>[[-0.74,5],[-10,10]]));

ENDDOCUMENT();
