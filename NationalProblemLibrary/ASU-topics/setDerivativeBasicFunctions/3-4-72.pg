## DESCRIPTION
## Re-Write then Calculate a Derivative
## ENDDESCRIPTION

## KEYWORDS('derivative', 'polynomial')
## Tagged by YL

## DBsubject('Calculus')
## DBchapter('Differentiation')
## DBsection('Derivatives of Polynomial and Exponential Functions')
## Date('')
## Author('')
## Institution('ASU')
## TitleText1('Calculus')
## EditionText1('5e')
## AuthorText1('Stewart')
## Section1('3.1')
## Problem1('')

DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());

$a = non_zero_random(-10,10,1);
$b = non_zero_random(-10,10,1);
$c = non_zero_random(-10,10,1);
while ($a**2 == 1)
{
$a = non_zero_random(-10,10,1);
}
while ($b**2 == 1)
{
$b = non_zero_random(-10,10,1);
}
while ($c**2 == 1)
{
$c = non_zero_random(-10,10,1);
}
$d = random(-4,-1,1);
$f = random(1,4,1);

TEXT(EV2(<<EOT));
Suppose that
\( \displaystyle{f(x) = \frac{$a x^5 ? {$b} x^4 ? {$c} x}{x^3}}\).
Evaluate each of the following:
$BR
$BR
\( f'($d) \) = \{ans_rule(20) \}
$BR
EOT

$ans = 2*$a*$d + $b - 2*$c/(($d)**3);
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
\( f'($f) \) = \{ans_rule(20) \}
$BR
EOT

$ans = 2*$a*$f + $b - 2*$c/(($f)**3);
ANS(num_cmp($ans));


ENDDOCUMENT();
