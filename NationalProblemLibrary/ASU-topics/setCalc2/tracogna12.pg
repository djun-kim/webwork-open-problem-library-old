TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,5,1);

$funct = "sec($a*x)**7/(7*$a) - sec($a*x)^5/(5*$a)";

BEGIN_TEXT;

Evaluate the indefinite integral.
$BR \[ \int \tan^3 ($a x) \sec^5($a x)\,dx \]
$BR $BR Answer = \{ans_rule( 45) \} \(+C\)
END_TEXT

$ans = $funct;
ANS(fun_cmp($ans, mode=>"antider"));

