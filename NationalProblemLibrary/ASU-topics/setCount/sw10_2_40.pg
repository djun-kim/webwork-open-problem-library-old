loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=random(2,5,1);

if ($a == 2){
   $ans = 12*11/2;
   };
if ($a == 3){
   $ans = 12*11*10/(2*3);
   };
if ($a == 4){
   $ans = 12*11*10*9/(2*3*4);
   };
if ($a == 5){
   $ans = 12*11*10*9*8/(2*3*4*5);
   };


BEGIN_TEXT
In how many ways can $a ice cream toppings be chosen from 12 available toppings?
$BR
Your answer is : \{ans_rule(20) \}
$BR
END_TEXT

ANS(std_num_cmp($ans));

