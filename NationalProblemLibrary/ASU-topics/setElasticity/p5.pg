#DESCRIPTION
##  Elasticity 
##ENDDESCRIPTION

##KEYWORDS('algebra', 'expeonential function')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=random(4, 8, 1);

BEGIN_TEXT
The demand function for a certain item is 
\( x = 200(80-p^2) \) 
$BR
(a) Evaluate the elasticity at $a.
\(E($a)=\) \{ans_rule(10) \}
$BR
(b) For what value of p, the demand is unitary.
\(p=\) \{ans_rule(10) \}
$BR
END_TEXT

$ans1 = "2*$a*$a/(80-$a*$a)";
$ans2 = sqrt(80/3);


ANS(num_cmp($ans1));
ANS(num_cmp($ans2));

ENDDOCUMENT();        # This should be the last executable line in the problem.

