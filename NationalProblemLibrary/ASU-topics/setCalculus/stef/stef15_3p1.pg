## Description  
##  Double integral
## Stewart 5e Section 15.3 problem 37
## EndDescription

DOCUMENT();
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "PGgraphmacros.pl",
           "Dartmouthmacros.pl");
 

## Do NOT show partial correct answers
$showPartialCorrectAnswers = 1;

$a = random(1,8);
$b = random(1,8);
$aa = $a**2;
$p = nicestring([$b],["\sqrt{x}"]);

TEXT(beginproblem());


BEGIN_TEXT
$BR
Consider the integral
\(\displaystyle \int_0^{$aa} \int_0^{$p}  f(x,y) dy dx \). 
Sketch the region of integration and change the order of integration.
 $PAR
\(\displaystyle \int_a^b
\int_{g_1(y)}^{g_2(y)} f(x,y)  dx dy \)

$BR
    \( a = \) \{ans_rule()\}   \( b = \) \{ans_rule()\} 

$BR
    \( g_1(y) = \) \{ans_rule()\}   \( g_2(y) = \) \{ans_rule()\} 

$PAR

END_TEXT


ANS(std_num_cmp(0)); ANS(std_num_cmp($b*$a));
ANS(function_cmp("y^2/$b**2","y") ); ANS(function_cmp("$aa","y") );

ENDDOCUMENT();