ANS(std_num_cmp(0)); ANS(std_num_cmp($n));
ANS(function_cmp("$a*x","x") ); ANS(function_cmp("$n*$a","x") );
ANS(multivar_function_cmp("0",["y","x"] ));
ANS(multivar_function_cmp("y-$a*x",["y","x"] ));

### 2. ####
ANS(std_num_cmp(0)); ANS(std_num_cmp($n*$a));
ANS(function_cmp("0","y") ); ANS(function_cmp("y/$a","y") );
ANS(multivar_function_cmp("0",["y","x"] ));
