##
## Description  
##  Double integral
## Stewart 5e Section 15.2 problem 34
## EndDescription

DOCUMENT();
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "PGgraphmacros.pl",
           "Dartmouthmacros.pl");
 

## Do NOT show partial correct answers
$showPartialCorrectAnswers = 1;

$a = random(2,8);
$c = random(1,8);
$d = random(1,8);
while ($d == $c) {$d = random(1,8);}

TEXT(beginproblem());


BEGIN_TEXT
$BR
Find the average value of \( f(x,y) = $a e^{ y} \sqrt{x+e^{ y}} \) over the
rectangle   \( R = [0,$d] \times [0,$c]  \).
 $BR


Average value =  \{ans_rule()\}   
END_TEXT


ANS(std_num_cmp(4*$a*(($d+exp($c))**(5/2)-($d+1)**(5/2)-exp(5*$c/2)+1)/(15*$c*$d))); 

ENDDOCUMENT();