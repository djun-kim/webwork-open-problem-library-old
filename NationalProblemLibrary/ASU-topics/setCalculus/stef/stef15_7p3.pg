DOCUMENT();
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "PGgraphmacros.pl",
           "Dartmouthmacros.pl");
 

## Do NOT show partial correct answers
$showPartialCorrectAnswers = 1;

$a = random(1,9);
$n = random(1,6,1);
$b = $n*$a;
$c = random(1,9);
$d = random(1,9);
$aa = $a**2;

$p1 = nicestring([$a],["x^2"]);
$p2 = nicestring([$d],["x"]);
$p3 = nicestring([$b],["x"]);

TEXT(beginproblem());


BEGIN_TEXT
$BR
Evaluate the triple integral 

\(\displaystyle \iiint_{E} (x + $c y) \, dV \) where \(E\) is bounded by
the parabolic cylinder \( y = $p1 \) and the planes
\( z = $p2, y = $p3, \) and \( z = 0 \).

  $PAR
\{ans_rule(40)\}

$PAR
END_TEXT


ANS(num_cmp($d*$n**4*$b*(5*$c*$b+6)/120));

ENDDOCUMENT();
