## Do NOT show partial correct answers
$showPartialCorrectAnswers = 1;

$a = random(1,8);
$b = random(1,8);
$aa = $a**2;
$p = nicestring([$b],["y"]);

TEXT(beginproblem());


BEGIN_TEXT
$BR
Express the integral 
\(\displaystyle \iiint_E f(x,y,z) dV \) as an iterated integral
in six different ways, where E is the solid bounded by
\( z =0,  z = $p \) and \( x^2 = $aa -y \).
 $PAR
1.
\(\displaystyle \int_a^b
\int_{g_1(x)}^{g_2(x)} \int_{h_1(x,y)}^{h_2(x,y)}f(x,y,z) dz dy dx \)

$BR
    \( a = \) \{ans_rule()\}   \( b = \) \{ans_rule()\} 

$BR
    \( g_1(x) = \) \{ans_rule()\}   \( g_2(x) = \) \{ans_rule()\} 

$BR
    \( h_1(x,y) = \) \{ans_rule()\}   \( h_2(x,y) = \) \{ans_rule()\} 
$PAR

2.
\(\displaystyle \int_a^b
\int_{g_1(y)}^{g_2(y)} \int_{h_1(x,y)}^{h_2(x,y)}f(x,y,z) dz dx dy \)

$BR
    \( a = \) \{ans_rule()\}   \( b = \) \{ans_rule()\} 

$BR
    \( g_1(y) = \) \{ans_rule()\}   \( g_2(y) = \) \{ans_rule()\} 

$BR
    \( h_1(x,y) = \) \{ans_rule()\}   \( h_2(x,y) = \) \{ans_rule()\} 
$PAR

3. 
\(\displaystyle \int_a^b
\int_{g_1(z)}^{g_2(z)} \int_{h_1(y,z)}^{h_2(y,z)}f(x,y,z) dx dy dz \)

$BR
    \( a = \) \{ans_rule()\}   \( b = \) \{ans_rule()\} 

$BR
    \( g_1(z) = \) \{ans_rule()\}   \( g_2(z) = \) \{ans_rule()\} 

$BR
    \( h_1(y,z) = \) \{ans_rule()\}   \( h_2(y,z) = \) \{ans_rule()\} 
$PAR

4. 
\(\displaystyle \int_a^b
\int_{g_1(y)}^{g_2(y)} \int_{h_1(y,z)}^{h_2(y,z)}f(x,y,z) dx dz dy \)

$BR
    \( a = \) \{ans_rule()\}   \( b = \) \{ans_rule()\} 

$BR
    \( g_1(y) = \) \{ans_rule()\}   \( g_2(y) = \) \{ans_rule()\} 

$BR
    \( h_1(y,z) = \) \{ans_rule()\}   \( h_2(y,z) = \) \{ans_rule()\} 
$PAR

5. 
\(\displaystyle \int_a^b
\int_{g_1(x)}^{g_2(x)} \int_{h_1(x,z)}^{h_2(x,z)}f(x,y,z) dy dz dx \)

$BR
    \( a = \) \{ans_rule()\}   \( b = \) \{ans_rule()\} 

$BR
    \( g_1(x) = \) \{ans_rule()\}   \( g_2(x) = \) \{ans_rule()\} 

$BR
    \( h_1(x,z) = \) \{ans_rule()\}   \( h_2(x,z) = \) \{ans_rule()\} 
$PAR

6. 
\(\displaystyle \int_a^b
\int_{g_1(z)}^{g_2(z)} \int_{h_1(x,z)}^{h_2(x,z)}f(x,y,z) dy dx dz \)

$BR
    \( a = \) \{ans_rule()\}   \( b = \) \{ans_rule()\} 

$BR
    \( g_1(z) = \) \{ans_rule()\}   \( g_2(z) = \) \{ans_rule()\} 

$BR
    \( h_1(x,z) = \) \{ans_rule()\}   \( h_2(x,z) = \) \{ans_rule()\} 
$PAR

END_TEXT

### 1. ###
ANS(std_num_cmp(-$a)); ANS(std_num_cmp($a));
ANS(function_cmp("0","x") ); ANS(function_cmp("$aa - x**2","x") );
ANS(multivar_function_cmp("0",["y","x"] ));
ANS(multivar_function_cmp("$b*y",["y","x"] ));

### 2. ####
ANS(std_num_cmp(0)); ANS(std_num_cmp($aa));
ANS(function_cmp("-sqrt($aa-y)","y") ); ANS(function_cmp("sqrt($aa-y)","y") );
ANS(multivar_function_cmp("0",["y","x"] ));
ANS(multivar_function_cmp("$b*y",["y","x"] ));

####  3. ####
ANS(std_num_cmp(0)); ANS(std_num_cmp($b*$aa));
ANS(function_cmp("z/$b","z") ); ANS(function_cmp("$aa","z") );
ANS(multivar_function_cmp("-sqrt($aa-y)",["y","z"] ));
ANS(multivar_function_cmp("sqrt($aa-y)",["y","z"] ));


#### 4. ####
ANS(std_num_cmp(0)); ANS(std_num_cmp($aa));
ANS(function_cmp("0","y") ); ANS(function_cmp("$b*y","y") );
ANS(multivar_function_cmp("-sqrt($aa-y)",["y","z"] ));
ANS(multivar_function_cmp("sqrt($aa-y)",["y","z"] ));


#####  5. #####
ANS(std_num_cmp(-$a)); ANS(std_num_cmp($a));
ANS(function_cmp("0","x") ); ANS(function_cmp("$b*($aa-x^2)","x") );
ANS(multivar_function_cmp("z/$b",["x","z"] ));
ANS(multivar_function_cmp("$aa - x^2",["x","z"] ));

##### 6. #####
ANS(std_num_cmp(0)); ANS(std_num_cmp($b*$aa));
ANS(function_cmp("-sqrt($aa-z/$b)","z") ); ANS(function_cmp("sqrt($aa-z/$b)","z") );
ANS(multivar_function_cmp("z/$b",["x","z"] ));
ANS(multivar_function_cmp("$aa-x^2",["x","z"] ));




